{
    "name": "cybercog/php-pushwoosh",
    "description": "A PHP Library to easily send push notifications with the Pushwoosh REST Web Services.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cybercog",
        "gomoob",
        "notification",
        "push",
        "pushwoosh",
        "sdk",
        "send",
        "mobile",
        "rest"
    ],
    "authors": [
        {
            "name": "Baptiste Gaillard",
            "email": "baptiste.gaillard@gomoob.com",
            "role": "Developer"
        },
        {
            "name": "Simon Baudry",
            "email": "simon.baudry@gomoob.com",
            "role": "Developer"
        },
        {
            "name": "Daniil Elizarov",
            "homepage": "https://github.com/dvpost",
            "role": "Developer"
        },
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Gomoob\\": "src/main/php/Gomoob"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gomoob\\": "src/test/php/Gomoob"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "^0.3.2",
        "pdepend/pdepend": "^2.2.4",
        "phpdocumentor/phpdocumentor": "^2.9.0",
        "phploc/phploc": "^3.0.1",
        "phpmd/phpmd": "^2.4.3",
        "phpunit/phpunit": "^5.5.4|^6.0",
        "satooshi/php-coveralls": "^1.0.1",
        "sebastian/phpcpd": "^2.0.4",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/yaml": "^3.1.4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
